type SkillType {
  id: ID! @id
  name: String!
  description: String!
  contains: [Skill!]!
  createdAt: DateTime! @createdAt
  updatedAt: DateTime! @updatedAt
}

type Skill {
  id: ID! @id
  name: String!
  description: String!
  type: SkillType! 
  tradedIn: [Trade!]!
  ratedIn: [Skill!]!
  createdAt: DateTime! @createdAt
  updatedAt: DateTime! @updatedAt
}